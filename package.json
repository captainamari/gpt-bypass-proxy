{
  "name": "gpt-bypass-proxy-server",
  "version": "1.0.0",
  "description": "proxy server for GPT Bypass Chrome Extension",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "build": "npm run test && npm run lint",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "docker:build": "docker build -t gpt-bypass-proxy .",
    "docker:run": "docker run -p 10800:10800 gpt-bypass-proxy",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop gpt-bypass-proxy",
    "pm2:restart": "pm2 restart gpt-bypass-proxy",
    "setup": "npm install && npm run build"
  },
  "keywords": [
    "proxy",
    "http-proxy",
    "https-proxy",
    "gpt-bypass",
    "ai-services",
    "chrome-extension"
  ],
  "author": "GPT Bypass Team",
  "license": "MIT",
  "dependencies": {
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "dotenv": "^16.3.1",
    "rate-limiter-flexible": "^3.0.8"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {},
  "bugs": {},
  "homepage": ""
}
